<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

<!-- Aluno -->
	<bean id="alunoNegocio" class="br.ufg.inf.apsi.escola.componentes.admc.negocio.bean.AlunoNegocioBean">
        <constructor-arg ref="alunoRepositorio"/>
    </bean>

<!-- Docente -->
	<bean id="docenteNegocio" class="br.ufg.inf.apsi.escola.componentes.admc.negocio.bean.DocenteNegocioBean">
        <constructor-arg ref="docenteRepositorio"/>
    </bean>

<!-- Disciplina -->
	<bean id="disciplinaNegocio" class="br.ufg.inf.apsi.escola.componentes.admc.negocio.bean.DisciplinaNegocioBean">
        <constructor-arg ref="disciplinaRepositorio"/>
    </bean>

<!-- Curso  -->
	<bean id="cursoNegocio" class="br.ufg.inf.apsi.escola.componentes.admc.negocio.bean.CursoNegocioBean">
        <constructor-arg ref="cursoRepositorio"/>
    </bean>

<!-- MatriculaTurma -->
	<bean id="matriculaTurmaNegocio" class="br.ufg.inf.apsi.escola.componentes.admc.negocio.bean.MatriculaTurmaNegocioBean">
        <constructor-arg ref="matriculaTurmaRepositorio"/>
    </bean>

<!-- Turma -->
	<bean id="turmaNegocio" class="br.ufg.inf.apsi.escola.componentes.admc.negocio.bean.TurmaNegocioBean">
        <constructor-arg ref="turmaRepositorio"/>
    </bean>

<!-- ProgramaDisciplina -->
	<bean id="programaDisciplinaNegocio" class="br.ufg.inf.apsi.escola.componentes.admc.negocio.bean.ProgramaDisciplinaNegocioBean">
        <constructor-arg ref="programaDisciplinaRepositorio"/>
    </bean>

<!-- PreMatriculaDisciplina -->
	<bean id="preMatriculaDisciplinaNegocio" class="br.ufg.inf.apsi.escola.componentes.admc.negocio.bean.PreMatriculaDisciplinaNegocioBean">
        <constructor-arg ref="preMatriculaDisciplinaRepositorio"/>
    </bean>

</beans>
