<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">


<!-- Aluno -->
<bean id="localAlunoService" class="br.ufg.inf.apsi.escola.componentes.admc.servico.local.LocalAlunoService">
        <constructor-arg ref="alunoNegocio"/>
</bean>

<!-- Docente -->
<bean id="localDocenteService" class="br.ufg.inf.apsi.escola.componentes.admc.servico.local.LocalDocenteService">
        <constructor-arg ref="docenteNegocio"/>
</bean>

<!-- Disciplina -->
<bean id="localDisciplinaService" class="br.ufg.inf.apsi.escola.componentes.admc.servico.local.LocalDisciplinaService">
        <constructor-arg ref="disciplinaNegocio"/>
</bean>

<!-- Curso  -->
<bean id="localCursoService" class="br.ufg.inf.apsi.escola.componentes.admc.servico.local.LocalCursoService">
        <constructor-arg ref="cursoNegocio"/>
</bean>

<!-- MatriculaTurma -->
<bean id="localMatriculaTurmaService" class="br.ufg.inf.apsi.escola.componentes.admc.servico.local.LocalMatriculaTurmaService">
        <constructor-arg ref="matriculaTurmaNegocio"/>
</bean>

<!-- Turma -->
<bean id="localTurmaService" class="br.ufg.inf.apsi.escola.componentes.admc.servico.local.LocalTurmaService">
        <constructor-arg ref="turmaNegocio"/>
</bean>

<!-- ProgramaDisciplina -->
<bean id="localProgramaDisciplinaService" class="br.ufg.inf.apsi.escola.componentes.admc.servico.local.LocalProgramaDisciplinaService">
        <constructor-arg ref="promaDisciplinaNegocio"/>
</bean>

<!-- PreMatriculaDisciplina -->
<bean id="localPreMatriculaDisciplinaService" class="br.ufg.inf.apsi.escola.componentes.admc.servico.local.LocalPreMatriculaDisciplinaService">
        <constructor-arg ref="preMatriculaDisciplinaNegocio"/>
</bean>

</beans>
