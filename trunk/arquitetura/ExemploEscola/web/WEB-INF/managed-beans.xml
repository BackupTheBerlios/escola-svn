        <managed-bean-name>AcessoNegado</managed-bean-name>
        <managed-bean-class>exemploescola.AcessoNegado</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>Principal</managed-bean-name>
        <managed-bean-class>exemploescola.Principal</managed-bean-class>
